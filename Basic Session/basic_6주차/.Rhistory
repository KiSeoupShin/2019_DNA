MEAN <- function(x, na.rm = FALSE){
if (na.rm == TRUE){
x <- na.omit(x) #NA값을 제거하는 함수
Mean <- sum(x)/length(x)
return(Mean)
} else if(na.rm == FALSE){
if (sum(is.na(x) %in% TRUE)>=1){
print("vector has NA")
} else {
Mean <- sum(x)/length(x)
return(Mean)
}
}
}
